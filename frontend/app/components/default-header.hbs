<div class="dark:bg-[#021D4A]">
    <nav class="flex flex-wrap justify-between items-center p-4 mx-auto max-w-screen-xl">
        {{!-- Logo --}}
        <div>
            <a href="/">
                <Logo />
            </a>
        </div>

        {{!-- Desktop and Tablet Navigation --}}
        <div class="hidden md:flex md:flex-wrap md:items-center md:space-x-8 lg:space-x-8">
            <a href="/service"
                class="font-heading font-bold text-black hover:text-yellow-400 dark:text-yellow-400 hover:dark:text-white">
                Our Service
            </a>
            <a href="/product"
                class="font-heading font-bold text-black hover:text-yellow-400 dark:text-yellow-400 hover:dark:text-white">
                Our Product
            </a>
            <a href="/blogs"
                class="font-heading font-bold text-black hover:text-yellow-400 dark:text-yellow-400 hover:dark:text-white">
                Blogs
            </a>
            <a href="/careers"
                class="font-heading font-bold text-black hover:text-yellow-400 dark:text-yellow-400 hover:dark:text-white">
                Careers
            </a>
            <a href="/about"
                class="font-heading font-bold text-black hover:text-yellow-400 dark:text-yellow-400 hover:dark:text-white">
                About Us
            </a>
            <button
                class="font-heading font-semibold px-6 py-2 rounded-lg font-medium text-sm transition-colors bg-[#021D4A] text-white hover:bg-yellow-400 focus:ring-4 focus:ring-yellow-400 dark:bg-yellow-400 dark:text-black dark:hover:bg-white dark:focus:ring-yellow-400"
                type="button" {{on "click" this.toggleContactModal}}>
                Contact Us
            </button>

            {{!-- Theme Toggle for Medium and Large Devices --}}
            <button type="button" class="rounded-full p-2 bg-gray-100 dark:bg-gray-700 focus:outline-none" {{on "click"
                this.toggleTheme}} aria-label="Toggle Theme">
                {{#if (eq this.theme "light")}}
                <svg class="w-6 h-6" fill="black" viewBox="0 0 64 64">
                    <path
                        d="M63.4374046,38.4606323c-0.4258003-0.2060013-0.9384995-0.0848999-1.2276001,0.2910995c-1.2803001,1.6650009-2.9452972,3.2001991-4.9501991,4.5625c-11.6180992,7.8808022-27.544899,4.9248009-35.4971008-6.5956993c-7.8154011-11.3204002-4.9902-26.9736996,6.2968998-34.8936005c0.3799-0.2666,0.5263996-0.7588,0.3544998-1.1895001c-0.1728001-0.4316-0.625-0.6835-1.0771008-0.6181c-4.6474991,0.6953-9.2070999,2.4902-13.1875,5.1893997C6.9706059,10.0759325,2.1415057,17.427433,0.5526057,25.9079323c-1.582,8.4414024,0.2402,16.9932022,5.1308002,24.0791016c6.2872,9.1054993,16.4864006,14.0058975,26.8554993,14.0058975c6.3173981,0,12.6982994-1.819397,18.2939987-5.6161995c6.5800018-4.461998,11.2461014-11.1298981,13.1406021-18.7753983C64.0877075,39.142334,63.8631058,38.6657333,63.4374046,38.4606323z" />
                </svg>
                {{else}}
                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5"
                    stroke="currentColor" class="w-6 h-6">
                    <path stroke-linecap="round" stroke-linejoin="round"
                        d="M12 3v1.5M12 19.5V21M4.219 4.219l1.06 1.06M18.72 18.72l1.06 1.06M3 12h1.5M19.5 12H21M4.219 19.781l1.06-1.06M18.72 5.28l1.06-1.06M8.25 12a3.75 3.75 0 107.5 0 3.75 3.75 0 00-7.5 0z" />
                </svg>
                {{/if}}
            </button>
        </div>

        {{!-- Mobile Menu Button --}}
        <button class="md:hidden text-black dark:text-white" type="button" {{on "click" this.toggleDrawer}}>
            <svg xmlns="http://www.w3.org/2000/svg" class="w-6 h-6" fill="none" viewBox="0 0 24 24"
                stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h16" />
            </svg>
        </button>
    </nav>

    {{!-- Mobile Drawer --}}
    {{#if this.isDrawerOpen}}
    <div class="fixed inset-0 z-50 bg-black bg-opacity-50 transition-opacity duration-300 ease-in-out"
        aria-hidden="true">
        <div class="bg-white dark:bg-gray-900 w-80 h-full shadow-lg p-6 transform transition-transform duration-300 ease-in-out"
            style="transform: translateX(0);" role="dialog" aria-modal="true" aria-label="Mobile Navigation Drawer">
            {{!-- Close Button --}}
            <button type="button" class="text-gray-700 dark:text-gray-300 mb-6 focus:outline-none"
                aria-label="Close Menu" {{on "click" this.toggleDrawer}}>
                <svg xmlns="http://www.w3.org/2000/svg" class="w-6 h-6" fill="none" viewBox="0 0 24 24"
                    stroke="currentColor">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12" />
                </svg>
            </button>

            {{!-- Navigation Links --}}
            <nav class="space-y-4">
                <a href="/service"
                    class="block py-2 px-4 text-gray-800 dark:text-gray-100 hover:bg-gray-100 dark:hover:bg-gray-800 rounded transition focus:outline-none focus:ring focus:ring-blue-500"
                    aria-label="Our Service">Our Service</a>
                <a href="/product"
                    class="block py-2 px-4 text-gray-800 dark:text-gray-100 hover:bg-gray-100 dark:hover:bg-gray-800 rounded transition focus:outline-none focus:ring focus:ring-blue-500"
                    aria-label="Our Product">Our Product</a>
                <a href="/blogs"
                    class="block py-2 px-4 text-gray-800 dark:text-gray-100 hover:bg-gray-100 dark:hover:bg-gray-800 rounded transition focus:outline-none focus:ring focus:ring-blue-500"
                    aria-label="Blogs">Blogs</a>
                <a href="/careers"
                    class="block py-2 px-4 text-gray-800 dark:text-gray-100 hover:bg-gray-100 dark:hover:bg-gray-800 rounded transition focus:outline-none focus:ring focus:ring-blue-500"
                    aria-label="Careers">Careers</a>
                <a href="/about"
                    class="block py-2 px-4 text-gray-800 dark:text-gray-100 hover:bg-gray-100 dark:hover:bg-gray-800 rounded transition focus:outline-none focus:ring focus:ring-blue-500"
                    aria-label="About Us">About Us</a>
<button
    class="font-heading font-semibold px-6 py-2 rounded-lg font-medium text-sm transition-colors bg-[#021D4A] text-white hover:bg-yellow-400 focus:ring-4 focus:ring-yellow-400 dark:bg-yellow-400 dark:text-black dark:hover:bg-white dark:focus:ring-yellow-400"
    type="button" {{on "click" this.toggleContactModal}}>
    Contact Us
</button>
            </nav>

            {{!-- Theme Toggle --}}
            <div class="mt-6 flex items-center justify-center">
                <button type="button" class="rounded-full p-2 bg-gray-100 dark:bg-gray-700 focus:outline-none"
                    {{on "click" this.toggleTheme}} aria-label="Toggle Theme">
                    {{#if (eq this.theme "light")}}
                    <svg class="w-6 h-6" fill="black" viewBox="0 0 64 64">
                        <path
                            d="M63.4374046,38.4606323c-0.4258003-0.2060013-0.9384995-0.0848999-1.2276001,0.2910995c-1.2803001,1.6650009-2.9452972,3.2001991-4.9501991,4.5625c-11.6180992,7.8808022-27.544899,4.9248009-35.4971008-6.5956993c-7.8154011-11.3204002-4.9902-26.9736996,6.2968998-34.8936005c0.3799-0.2666,0.5263996-0.7588,0.3544998-1.1895001c-0.1728001-0.4316-0.625-0.6835-1.0771008-0.6181c-4.6474991,0.6953-9.2070999,2.4902-13.1875,5.1893997C6.9706059,10.0759325,2.1415057,17.427433,0.5526057,25.9079323c-1.582,8.4414024,0.2402,16.9932022,5.1308002,24.0791016c6.2872,9.1054993,16.4864006,14.0058975,26.8554993,14.0058975c6.3173981,0,12.6982994-1.819397,18.2939987-5.6161995c6.5800018-4.461998,11.2461014-11.1298981,13.1406021-18.7753983C64.0877075,39.142334,63.8631058,38.6657333,63.4374046,38.4606323z" />
                    </svg>
                    {{else}}
                    <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5"
                        stroke="currentColor" class="w-6 h-6">
                        <path stroke-linecap="round" stroke-linejoin="round"
                            d="M12 3v1.5M12 19.5V21M4.219 4.219l1.06 1.06M18.72 18.72l1.06 1.06M3 12h1.5M19.5 12H21M4.219 19.781l1.06-1.06M18.72 5.28l1.06-1.06M8.25 12a3.75 3.75 0 107.5 0 3.75 3.75 0 00-7.5 0z" />
                    </svg>
                    {{/if}}
                </button>
            </div>
        </div>
    </div>
    {{/if}}

    {{!-- Include the contact-us component --}}
    <ContactUs @openModal={{this.globalMethods.isModalOpen}} @onCloseModal={{this.globalMethods.toggleModalContact}} />
</div>